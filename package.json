{
  "name": "duttywinner",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "images": "node scripts/images.js",
    "ios:pod": "cd ios && pod install && cd ..",
    "ios:clear": "rm -rf ios/build",
    "android:run": "react-native run-android",
    "android:run-release": "react-native run-android --variant=release",
    "android:clean": "cd android && ./gradlew clean && cd ..",
    "android:releaseDebug": "cd android && ENVFILE=.env.development && ./gradlew assembleDebug && cd ..",
    "android:releaseDev": "cd android && ENVFILE=.env.development && ./gradlew assembleRelease && cd ..",
    "android:releaseProd": "cd android && ENVFILE=.env.production && ./gradlew assembleRelease && cd ..",
    "android:fastlaneACProd": "cd android && fastlane app_center_prod --env prod && cd ..",
    "android:fastlaneACDev": "cd android && fastlane app_center_dev && cd ..",
    "ios:fastlaneTF": "cd ios && fastlane tflight --env prod && cd ..",
    "ios:fastlaneACDev": "cd ios && fastlane app_center && cd .."
  },
  "dependencies": {
    "@invertase/react-native-apple-authentication": "^2.1.5",
    "@react-native-clipboard/clipboard": "^1.9.0",
    "@react-native-community/async-storage": "^1.12.1",
    "@react-native-google-signin/google-signin": "^7.0.4",
    "@react-navigation/bottom-tabs": "^6.0.9",
    "@react-navigation/material-top-tabs": "^6.0.6",
    "@react-navigation/native": "^6.0.6",
    "@react-navigation/stack": "^6.0.11",
    "axios": "^0.24.0",
    "jwt-decode": "^3.1.2",
    "react": "17.0.2",
    "react-native": "0.66.4",
    "react-native-config": "^1.4.5",
    "react-native-drop-shadow": "0.0.4",
    "react-native-fbsdk-next": "^4.3.0",
    "react-native-flash-message": "^0.2.0",
    "react-native-fs": "^2.18.0",
    "react-native-gesture-handler": "^2.1.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-otp-inputs": "^7.1.1",
    "react-native-safe-area-context": "^3.3.2",
    "react-native-screens": "^3.10.1",
    "react-native-splash-screen": "^3.3.0",
    "react-redux": "^7.2.6",
    "redux": "^4.1.2",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.4.1",
    "validate.js": "^0.13.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "babel-jest": "^26.6.3",
    "eslint": "7.14.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.66.2",
    "react-test-renderer": "17.0.2"
  },
  "jest": {
    "preset": "react-native"
  }
}
